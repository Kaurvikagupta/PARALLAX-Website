{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/styles.css",
      "headers": { "cache-control": "s-maxage=31536000, max-age=0, public" },
      "continue": true
    },
    {
      "src": "/(.*\\.(?:png|jpg|gif|svg))$",
      "headers": { "cache-control": "s-maxage=31536000, max-age=0, public" },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
