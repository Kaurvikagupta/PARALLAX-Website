{
  "version": 2,
  "routes": [
    {
      "src": "/styles.css",
      "headers": {
        "cache-control": "s-maxage=31536000, max-age=0, public"
      },
      "continue": true
    },
    {
      "src": "/(.*\\.(?:png|jpg|gif|svg))$",
      "headers": {
        "cache-control": "s-maxage=31536000, max-age=0, public"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
